{
    "presets": [
        [ "@babel/preset-env", {
            "modules": false,
            "targets": {
                "browsers": [
                    "last 2 Chrome versions",
                    "last 2 Firefox versions",
                    "last 2 Safari versions",
                    "last 2 iOs versions",
                    "last 2 Android versions",
                    "last 2 ChromeAndroid versions",
                    "ie 11"
                ]
            }
        }

        ],
        "@babel/preset-react"
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"]
}